{
    "name": "danidoble/translation",
    "description": "translate with json file",
    "type": "plugin",
    "homepage": "https://github.com/danidoble/translation",
    "license": "MIT",
    "suggest": {
        "danidoble/gf": "*"
    },
    "keywords": [
        "danidoble",
        "translation"
    ],
    "authors": [
        {
            "name": "danidoble",
            "email": "dani.doble.d@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*"
    },
    "autoload": {
        "files": [ "Src/Global.php" ],
        "psr-4": {
            "Danidoble\\Translation\\": "Src\\"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "version": "v0.0.2"
}
